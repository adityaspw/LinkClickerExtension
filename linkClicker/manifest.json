{
    "name": "Link Klicker",  

    "description": "Click all the links of a webpage",

    "version": "1.0",

    "manifest_version" : 3,
    
   
    "background" : {
        "service_worker": "background.js"
    },

    "permissions":[
        "activeTab",
        "scripting"
    ],

    "action" : {
        "default_popup":"popup.html"
    }
}